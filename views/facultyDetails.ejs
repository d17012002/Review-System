<%- include("./partials/header.ejs") %>

<link rel="stylesheet" href="/css/facultyDetails.css">

<body>
  <video autoplay muted loop id="myVideo">
    <source src="Assets/bg.mp4" type="video/mp4" />
  </video>
    <div class="fluid-container">
      
        <div class="header">
            <h4>Faculties Contact Information</h4>
            <hr>
            <p class="subHeading">Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an and scrambled it to make a type specimen book.It is a long established fact that a reader will be distracted by the readable content of a page when looking at its layout.</p>
          </div>
          <br>
            <div class="searchFaculty">
            <input type="text" class="input" id="inputfac" placeholder="Search Faculty...">
            <button><i class="fas fa-search fa-1x"></i></button>
  
        </div>
        <br>
        <div class="row">
            <div class="col-md-12">
                <div class="table-wrap">
                    <table class="table table-bordered table-dark table-hover">
                      
                        <tr>
                          <th>Emp ID</th>
                          <th>Name</th>
                          <th>Mobile No</th>
                          <th>Email ID</th>
                        </tr>
                      
                      
                        <% for(var i=0; i<details.length; i++) { %>
                        
                        <tr class="faculty-row">
                          <td class="faculty-empid-row" scope="row"> <%=details[i].empID %> </td>
                          <td class="faculty-name-row"> <%= details[i].name %> </td>
                          <td> <%= details[i].mob %> </td>
                          <td> <%= details[i].email_ID %> </td>
                        </tr>

                        <% } %>
                        
                  
                    </table>
                </div>
            </div>
        </div>
    </div>
    <br><br>
    <script src="javaScript/facultyDetails.js"></script>
</body>


<%- include("./partials/footer.ejs") %>