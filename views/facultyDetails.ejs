<%- include("./partials/header.ejs") %>

<link rel="stylesheet" href="/facultyDetails.css">

<body>
  <video autoplay muted loop id="myVideo">
    <source src="Assets/bg.mp4" type="video/mp4" />
  </video>
    <div class="fluid-container">
      
        <div class="header">
          
            <h4>Faculties Contact Information</h4>
          
          
          <hr>
         <p class="subHeading">Reviews of all these faculties are dropped by your friends and seniors. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an and scrambled it to make a type specimen book.It is a long established fact that a reader will be distracted by the readable content of a page when looking at its layout.</p>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="table-wrap">
                    <table class="table table-bordered table-dark table-hover">
                      <thead>
                        <tr>
                          <th>Emp ID</th>
                          <th>Name</th>
                          <th>Mobile No</th>
                          <th>Email ID</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for(var i=0; i<details.length; i++) { %>

                        <tr>
                          <td scope="row"> <%=details[i].empID %> </td>
                          <td> <%= details[i].name %> </td>
                          <td> <%= details[i].mob %> </td>
                          <td> <%= details[i].email_ID %> </td>
                        </tr>

                        <% } %>
                        
                      </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <br><br>
</body>


<%- include("./partials/footer.ejs") %>