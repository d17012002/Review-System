<%- include("./partials/header.ejs") %>

<style>
  body {
    background: rgb(19, 19, 19);
  }

  .userlist {
    border: 0.9px solid #2e2d2d;
    align-content: left;
    text-align: left;
    line-height: 20px;
    color: white;
    position: relative;
    top: 20px;
    padding: 20px;
    border-radius: 10px;
    width: 100%;
    background: rgb(19, 19, 19);
    margin-bottom: 2px;
  }

  h4 {
    margin-top: 30px;
    color: ivory;
  }

  .footer {
    width: 100%;
    padding: 50px;
    font-size: small;
    color: rgba(187, 187, 187, 0.315);
  }

  h6 {
    margin-top: 30px;
    color: ivory;
  }

  .fas {
    padding-left: 12px;
  }

  .subHeading {
    color: rgb(107, 104, 104);
  }
</style>

  <body>
    <div class="container">
      <h6>Total Users : <%= num %>
      </h6>
    </div>
    <div>
      <center>
        <div class="col-lg-6 col-sm-12 l1">
          <h4>People who used this Web Application</h4>
          <p class="subHeading">All these users are students of VIT Bhopal</p>
          <div class="userlist">
            <%for(var i=0; i<key.length; i++){%>
              <p>
                <%=i+1%>. <%= key[i].name %> - <%= key[i].email %><i class="fas fa-user-circle"></i>
              </p>
              <%}%>
          </div>
          <div class="footer">
            <br>
            <p class="subHeading"> Copyright 2022 | Anurag Kumar </p>
          </div>
      </center>
    </div>
    </div>
  </body>

  <%- include("./partials/footer.ejs") %>